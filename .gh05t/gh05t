#!/bin/bash
exit_on_signal_SIGINT() {
echo ""
clear 
echo " Trying To Exit"
sleep 5
exit 0
}
exit_on_signal_SIGTERM() {
echo ""
clear 
echo " Trying To Exit"
sleep 5
exit 0
}
trap exit_on_signal_SIGINT SIGINT
trap exit_on_signal_SIGTERM SIGTERM
function breakme () {
if [ -d /data/data/com.termux/files/usr/cookies ]; then
echo ""
clear
else
clear
echo " cookies not found"
echo " please update"
exit
fi
ping -c 1 google.com
Return="$?"
if [ "$Return" -eq "0" ]; then
echo ""
clear
else
clear
echo ""
echo " Please On Your Mobile Data"
sleep 8
clear
fi
}
function update-info () {
banners
sleep 3
pkg install git -y
pkg install python -y
dpkg -s python &> /dev/null # GH05T HUNTER5
if [ $? -eq 0 ]; then
echo ""
clear
else
echo ""
pkg install python 
clear
fi
dpkg -s git &> /dev/null # GH05T HUNTER5
if [ $? -eq 0 ]; then
echo ""
clear
else
echo ""
pkg install git 
clear
fi
cd $HOME
git clone https://github.com/GH05T-HUNTER5/GH05T-INSTA
cd GH05T-INSTA
python update.py
exit
}
