value="/data/data/com.termux/files/usr/cookies/password-6.txt"
elif [ $4 = 7 ]; then
value="/data/data/com.termux/files/usr/cookies/password-7.txt"
elif [ $4 = 8 ]; then
value="/data/data/com.termux/files/usr/cookies/password-8.txt"
elif [ $4 = 9 ]; then
value="/data/data/com.termux/files/usr/cookies/password-9.txt"
elif [ $4 = 10 ]; then
