exit
else
echo " Something Wrong"
exit
fi
else
echo ""
clear
fi
if [ $3 = "--passadd" ]; then
echo ""
if [ $1 = "--user" ]; then
echo ""
cp -r $4 /data/data/com.termux/files/usr/cookies/
