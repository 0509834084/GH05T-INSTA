error-info
clear
check-info
clear
exit
elif [ $option = "05" ] || [ $option = "5" ]; then
echo ""
clear
developer-info
clear
main-loop
clear
elif [ $option = "00" ] || [ $option = "0" ]; then
echo ""
clear
exit
elif [ $option = "99" ]; then
echo ""
clear
help-info
loop-infos
exit
else
echo ""
clear
echo " Something Wrong"
sleep 3
clear
main-loop
exit
fi
clear
main-loop
}
main-loop
